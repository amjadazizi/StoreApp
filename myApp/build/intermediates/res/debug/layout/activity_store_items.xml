<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/clouds">





    <FrameLayout
        android:id="@+id/lll"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation="horizontal"
        android:background="@color/clouds"

        >

        <ImageView
            android:id="@+id/imgItemPic"
            android:scaleType="fitXY"
            android:layout_width="match_parent"
            android:layout_height="198dp"
            android:src="@drawable/ic_image_white"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/peter_river"
            />

    </FrameLayout>



    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnAddImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        fab:fab_colorNormal="@color/white"
        fab:fab_addButtonColorNormal="@color/alizarin"
        fab:fab_title="Infomation"
        app:fab_size="normal"
        android:layout_alignParentRight="true"
        android:layout_marginTop="163dp"
        />


    <RelativeLayout
        android:id="@+id/kaka"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/lll"
        android:background="@color/transparent"

        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="104dp"
                android:layout_weight="1"
                android:orientation="vertical">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="Store Items"
                    android:textColor="@color/black"
                    android:layout_marginBottom="10dp"/>


                <View
                    android:layout_width="fill_parent"
                    android:layout_height="4dp"
                    android:layout_alignParentBottom="true"
                    android:background="@color/peter_river" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/itemButtons"
                android:layout_width="fill_parent"
                android:layout_height="64dp"
                android:clickable="true"
                android:layout_weight="1">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:layout_weight="1">

                        <Button
                            android:id="@+id/btnScanItem"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:icon="@drawable/ic_store_items"
                            android:background="@drawable/selector_button_pressed"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:layout_weight="2"
                        android:layout_marginLeft="51dp">

                        <com.storeapp.ui.FloatingEditText
                            android:singleLine="true"

                            android:id="@+id/editBarcode"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:hint="Barcode"
                            android:layout_marginRight="10dp"
                            android:layout_weight="1"
                            android:textColor="@color/black"
                            />

                        <com.storeapp.ui.FloatingEditText
                            android:id="@+id/editEan"
                            android:singleLine="true"
                            android:textColor="@color/black"

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:hint="Ean-Type"
                            android:layout_weight="2"
                            />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/itemSwitches"
                android:layout_width="fill_parent"
                android:layout_height="64dp"
                android:clickable="true"
                android:layout_weight="1">


                <com.storeapp.ui.FloatingEditText
                    android:id="@+id/editDecription"
                    android:singleLine="true"
                    android:textColor="@color/black"

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="104dp"
                    android:hint="Description"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    />



            </LinearLayout>


            <LinearLayout
                android:id="@+id/itemSwitches"
                android:layout_width="fill_parent"
                android:layout_height="64dp"
                android:clickable="true"
                android:layout_weight="1">




                <com.storeapp.ui.FloatingEditText
                    android:id="@+id/editAmount"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="104dp"
                    android:hint="Amount"
                    android:inputType="number"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    />

                <com.storeapp.ui.FloatingEditText
                    android:id="@+id/editCurAmountNstock"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:hint="Amount In Stock"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    android:inputType="none"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/itemProgress"
                android:layout_width="fill_parent"
                android:layout_height="64dp"
                android:clickable="true"
                android:layout_weight="1">



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:layout_weight="1">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:background="@drawable/ic_store_items"
                            android:visibility="invisible"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:layout_weight="2"
                        android:layout_marginLeft="51dp">

                        <com.storeapp.ui.FloatingEditText
                            android:id="@+id/editPurchaseprice"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:inputType="number"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:hint="Purchaseprice"
                            android:layout_marginRight="10dp"
                            android:layout_weight="1"
                            />

                        <com.storeapp.ui.FloatingEditText
                            android:id="@+id/editSellprice"
                            android:singleLine="true"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:hint="Sellprice"
                            android:layout_weight="1"
                            android:textColor="@color/black"
                            android:inputType="number"
                            />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <!-- RECTANGLE BUTTON -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_gravity="right"
                android:layout_weight="1"
                android:layout_marginLeft="104dp"

                >

                <Button
                    android:id="@+id/btnSaveItem"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/selector_button_pressed"
                    android:text="Save Item"
                    />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>



