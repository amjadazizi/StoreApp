    tools:context="com.storeapp.RegistrationFragmentBusiness.Activities.Store_Items"
    tools:ignore="MergeRootFrame" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#34AADC"
